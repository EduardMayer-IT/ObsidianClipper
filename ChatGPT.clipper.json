{
  "schemaVersion": "0.1.0",
  "name": "ChatGPT",
  "behavior": "create",
  "noteNameFormat": "{{title}}",
  "path": "Clippings/ChatGPT/{{selector:time|attr:datetime|date:\"YYYY\"}}",
  "noteContentFormat": "---\ntitle: {{title}}\nurl: {{url}}\nchat_id: {{url|split:\"/\"|slice:-1}}\nmodel: ChatGPT\ntags:\n  - chatgpt\n  - clipping\n---\n\n## ðŸ’¬ Konversation\n\n{{content}}\n\n---\n\n**ðŸ”— Quelle:** [ChatGPT Ã¶ffnen]({{url}})",
  "properties": [
    {
      "id": "title",
      "name": "Titel",
      "value": "{{title}}",
      "type": "text"
    },
    {
      "id": "url",
      "name": "URL",
      "value": "{{url}}",
      "type": "text"
    },
    {
      "id": "created",
      "name": "Erstellt",
      "value": "{{selector:time|attr:datetime|date:\"YYYY-MM-DD\"}}",
      "type": "date"
    },
    {
      "id": "chat_id",
      "name": "Chat-ID",
      "value": "{{url|split:\"/\"|slice:-1}}",
      "type": "text"
    },
    {
      "id": "model",
      "name": "Modell",
      "value": "{{selector:.text-token-text-secondary|text}}",
      "type": "text"
    },
    {
      "id": "timestamp",
      "name": "Zeitstempel",
      "value": "{{selector:time|attr:datetime}}",
      "type": "text"
    }
  ],
  "triggers": [
    "https://chatgpt.com/c/",
    "https://chat.openai.com/c/"
  ]
}
